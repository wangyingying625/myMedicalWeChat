page {
  background: #fbede4;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
text{
  color: #676767;
}
.img{
  height: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 35rpx;
}
.forText{
  text-align: center;
}
.img image{
  height: 200rpx;
  width: 200rpx;
  border-radius: 50%;
  border: 8rpx #f98e64 solid;
  display: block;
  margin-bottom: 10rpx;
}