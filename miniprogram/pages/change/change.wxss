page {
  background: #fbede4;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
text{
  color: #676767;
}
.img{
  height: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.forText{
  text-align: center;
}
.img image{
  height: 200rpx;
  width: 200rpx;
  border-radius: 50%;
  border: 8rpx #f98e64 solid;
  display: block;
}
.msg{
  display: flex;
  align-content: center;
  height: 90rpx;
  justify-content: center;
  margin-top: 30rpx;
}
.inpT{
  width: 65%;
  border: 2rpx solid #676767;
  border-radius: 10rpx;
  padding: 10rpx;
}
.submit{
  margin-top: 45rpx;
  background-color: #fbede4;
  color: #f98e64;
  width: 350rpx;
  margin-bottom: 35rpx;
  border: 4rpx solid #f98e64;
}
radio{
  color: #f98e64
}
.gender{
  margin-right: 15rpx;
  margin-bottom: 0;
  color :#f98e64;
  margin-left: 35rpx;
}
.radio-group{
  margin: 0;
  padding: 0;
  margin-right: 50rpx;
}